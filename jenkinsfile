node {
    stage('SCM Checkout') { // for display purposes
        // Get some code from a GitHub repository
       git credentialsId: 'githubcredential', url: 'https://github.com/ramyanarsupalli2109/coding-challenge-release-engineer.git'
    }
    stage('Maven Compile') { // for display purposes
        // Get some code from a GitHub repository
       def mvnHome = tool name: 'M3', type: 'maven'
	   def mvnCMD = "${mvnHome}/bin/mvn"
	   sh "${mvnCMD} package"
	} 
    stage('Mvn Package') { // for display purposes
        // Get some code from a GitHub repository
       def mvnHome = tool name: 'M3', type: 'maven'
	   def mvnCMD = "${mvnHome}/bin/mvn"
	   sh "${mvnCMD} clean package"
	}   

    stage('Build Docker Image') { // for display purposes
       sh 'docker build -t Dockerfile .'
	}   
	
}
